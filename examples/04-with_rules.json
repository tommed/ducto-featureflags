{
  "new_ui": {
    "variants": {
      "dev": 0,
      "alpha": 1,
      "beta": 2,
      "current": 3,
      "stable": 4
    },
    "rules": [
      { "if": { "env": "dev" }, "variant": "dev" },
      { "if": { "env": "alpha" }, "variant": "alpha"  },
      { "if": { "env": "beta" }, "variant": "beta" },
      { "if": { "env": "prod" }, "variant": "stable" }
    ],
    "defaultVariant": "current"
  }
}
